#include "unity.h"

void setUp(void) {
}

void tearDown(void) {
}

void test_addition(void) {
    TEST_ASSERT_EQUAL(5, add(2, 3));
}

void test_subtraction(void) {
    TEST_ASSERT_EQUAL(1, subtract(3, 2));
}

int main(void) {
    UNITY_BEGIN();
    RUN_TEST(test_addition);
    RUN_TEST(test_subtraction);
    return UNITY_END();
}